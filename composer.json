{
    "name": "iqtool/laravel-signoz",
    "description": "Laravel Signoz integration",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "illuminate/support": "*",
        "open-telemetry/sdk": "^1.0",
        "open-telemetry/exporter-otlp": "^1.0",
        "php-http/guzzle7-adapter": "^1.0",
        "open-telemetry/opentelemetry-auto-laravel": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Iqtool\\LaravelSignoz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iqtool\\LaravelSignoz\\Provider\\OpenTelemetryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    }
}


